    },
    {
      "name": "ü§ñ Node Robot",
      "program": "${workspaceFolder}/robots/robot.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "Start ü§ñ",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/robots/main.go"
    },
    {
      "name": "Start Project",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/backend/manage.py",
      "args": ["runserver"],
      "django": true,
      "justMyCode": true
    }
  ]
